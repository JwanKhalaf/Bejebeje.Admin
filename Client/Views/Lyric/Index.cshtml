@model ViewModels.Lyric.ArtistLyricsViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div>
    @foreach (var lyric in Model.Lyrics)
    {
      <h4>@lyric.Title</h4>
      <h6>@Html.ActionLink("Details", "Details", new { id = lyric.Id })</h6>
    }
</div>
<div>
    
    <a asp-action="Index">Back to List</a>
</div>
